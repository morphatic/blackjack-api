{
  "name": "blackjack-api",
  "description": "API for a react-based implementation of the classic game of 21",
  "version": "0.1.0",
  "homepage": "https://github.com/morphatic/blackjack-api",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Morgan Benton",
    "email": "morgan.benton@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
