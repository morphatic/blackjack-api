    "@feathersjs/transport-commons": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-mongoose": "^8.3.1",
    "helmet": "^4.3.1",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.11.10",
    "serve-favicon": "^2.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
